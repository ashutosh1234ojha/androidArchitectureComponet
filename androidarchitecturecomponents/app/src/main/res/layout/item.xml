            <!--<ImageView-->
                <!--android:layout_width="50dp"-->
                <!--android:layout_height="50dp"-->
                <!--app:imageUrl="@{myitem.imageUrl}"/>-->

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
